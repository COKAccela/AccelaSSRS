  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ReportType">
            <Value>=Parameters!ReportType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>--DECLARE @StartDate DATE = '2024-09-06';
--DECLARE @EndDate   DATE = '2025-09-08';
--DECLARE @ReportType VARCHAR(50) = 'Land Development';

;WITH Tiered AS (
    SELECT 
        CASE 
            WHEN TRY_CAST(b.B1_CHECKLIST_COMMENT AS MONEY) &lt;= 500000
                THEN 'Less than $500,000'
            WHEN TRY_CAST(b.B1_CHECKLIST_COMMENT AS MONEY) &gt; 500000 
                 AND TRY_CAST(b.B1_CHECKLIST_COMMENT AS MONEY) &lt;= 2000000
                THEN '$500,000 to $2,000,000'
            WHEN TRY_CAST(b.B1_CHECKLIST_COMMENT AS MONEY) &gt; 2000000  
                 AND TRY_CAST(b.B1_CHECKLIST_COMMENT AS MONEY) &lt;= 20000000
                THEN '$2,000,000 to $20,000,000'
            WHEN TRY_CAST(b.B1_CHECKLIST_COMMENT AS MONEY) &gt;= 20000000
                THEN 'More than $20,000,000'
            ELSE 'No Tier' 
        END AS ProjectValuation,

        COUNT(
            CASE 
                WHEN c.SD_APP_DES IS NULL 
                     OR c.SD_APP_DES NOT IN ('Issued','Permitted') 
                THEN 1 
            END
        ) AS Applied,

        COUNT(
            CASE 
                WHEN c.SD_APP_DES IN ('Issued','Permitted') THEN 1 
            END
        ) AS Issued,
		@ReportType AS ReportType,
		@StartDate AS StartDate,
		@EndDate AS EndDate

    FROM B1PERMIT a
    INNER JOIN BCHCKBOX b 
        ON a.SERV_PROV_CODE = b.SERV_PROV_CODE
       AND a.B1_PER_ID1 = b.B1_PER_ID1
       AND a.B1_PER_ID2 = b.B1_PER_ID2
       AND a.B1_PER_ID3 = b.B1_PER_ID3
    LEFT JOIN GPROCESS_HISTORY c 
        ON a.SERV_PROV_CODE = c.SERV_PROV_CODE
       AND a.B1_PER_ID1 = c.B1_PER_ID1
       AND a.B1_PER_ID2 = c.B1_PER_ID2
       AND a.B1_PER_ID3 = c.B1_PER_ID3
       AND c.SD_APP_DES IN ('Issued','Permitted')
       AND c.SD_APP_DD = (
            SELECT MIN(gh2.SD_APP_DD)
            FROM GPROCESS_HISTORY gh2
            WHERE gh2.SERV_PROV_CODE = c.SERV_PROV_CODE
              AND gh2.B1_PER_ID1 = c.B1_PER_ID1
              AND gh2.B1_PER_ID2 = c.B1_PER_ID2
              AND gh2.B1_PER_ID3 = c.B1_PER_ID3
              AND gh2.SD_APP_DES IN ('Issued','Permitted')
       )
    WHERE a.SERV_PROV_CODE = 'KNOXVILLE'
      AND a.REC_STATUS = 'A'
      AND b.B1_CHECKBOX_DESC = 'Estimated Cost (Job Value)'
      AND a.B1_PER_SUB_TYPE NOT IN ('Mechanical','Electrical','Plumbing')
      AND a.B1_FILE_DD BETWEEN @StartDate AND @EndDate
      AND a.B1_PER_TYPE = @ReportType
    GROUP BY 
        CASE 
            WHEN TRY_CAST(b.B1_CHECKLIST_COMMENT AS MONEY) &lt;= 500000
                THEN 'Less than $500,000'
            WHEN TRY_CAST(b.B1_CHECKLIST_COMMENT AS MONEY) &gt; 500000 
                 AND TRY_CAST(b.B1_CHECKLIST_COMMENT AS MONEY) &lt;= 2000000
                THEN '$500,000 to $2,000,000'
            WHEN TRY_CAST(b.B1_CHECKLIST_COMMENT AS MONEY) &gt; 2000000  
                 AND TRY_CAST(b.B1_CHECKLIST_COMMENT AS MONEY) &lt;= 20000000
                THEN '$2,000,000 to $20,000,000'
            WHEN TRY_CAST(b.B1_CHECKLIST_COMMENT AS MONEY) &gt;= 20000000
                THEN 'More than $20,000,000'
            ELSE 'No Tier' 
        END
)
SELECT 
    ProjectValuation,
    Applied,
    Issued,
	ReportType,
	StartDate,
	EndDate
FROM Tiered
ORDER BY 
    CASE 
        WHEN ProjectValuation = 'Less than $500,000' THEN 1
        WHEN ProjectValuation = '$500,000 to $2,000,000' THEN 2
        WHEN ProjectValuation = '$2,000,000 to $20,000,000' THEN 3
        WHEN ProjectValuation = 'More than $20,000,000' THEN 4
        ELSE 5
    END;</CommandText>
      </Query>
