          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="B1_MNAME">
          <DataField>B1_MNAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="B1_FULL_NAME">
          <DataField>b1_full_name</DataField>
