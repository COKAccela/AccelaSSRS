        </QueryParameters>
        <CommandText>--declare @Payment_Date_From Date = '03/01/2024'
--declare @Payment_Date_To Date = '03/14/2024'
--declare @Group varchar(100) = ''
--declare @Payment_Type varchar(100) = ''
drop table if exists #V_COK_RECORD_GROUP_FOR_FEE
select b1_alt_id as record_id, b1_per_group, b1_per_type, b1_per_group +'-'+ b1_per_type as RecGroup
into #V_COK_RECORD_GROUP_FOR_FEE
	from b1permit
	where serv_prov_code = 'KNOXVILLE'
SELECT 
#V_COK_RECORD_GROUP_FOR_FEE.RecGroup,
V_FEE_PAYMENT_HISTORY.FEE_DESCRIPTION,
V_FEE_PAYMENT_HISTORY.FEE_ACCT_CODE_1,
V_FEE_PAYMENT_HISTORY.AMOUNT,
V_FEE_PAYMENT_HISTORY.PAYMENT_METHOD,
V_FEE_PAYMENT_HISTORY.DATE_PAYMENT
FROM V_FEE_PAYMENT_HISTORY
INNER JOIN
#V_COK_RECORD_GROUP_FOR_FEE ON V_FEE_PAYMENT_HISTORY.RECORD_ID = #V_COK_RECORD_GROUP_FOR_FEE.record_id
WHERE
V_FEE_PAYMENT_HISTORY.AGENCY_ID = 'KNOXVILLE'
and TRY_CAST(V_FEE_PAYMENT_HISTORY.DATE_PAYMENT AS DATE)&gt;= @Payment_Date_From
AND TRY_CAST(V_FEE_PAYMENT_HISTORY.DATE_PAYMENT AS DATE) &lt;= @Payment_Date_To
    AND (#V_COK_RECORD_GROUP_FOR_FEE.RECGROUP = @Group OR ISNULL(@Group,'') ='')
    AND (V_FEE_PAYMENT_HISTORY.PAYMENT_METHOD IN (@Payment_Type) )
drop table if exists #V_COK_RECORD_GROUP_FOR_FEE</CommandText>
