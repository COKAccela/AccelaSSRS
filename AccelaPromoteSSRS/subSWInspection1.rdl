  <DataSources>
    <DataSource Name="Accela">
      <DataSourceReference>/NONPROD1/KNOXVILLE/KNOXVILLE</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>45b70c85-b007-466f-9c64-7f31c58d8053</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     a1.b1_alt_id, c.g6_compl_dd, 
'Status: ' + isnull(c.g6_status, '') + '; Activity#: ' + cast((c.g6_act_num) as varchar) + '; Activity Type: ' 
+ isnull(c.g6_act_typ, '') + '; By: ' + isnull(c.ga_lname, '') + ' ' + isnull(c.ga_fname, '')
+ '; Scheduled: '+isnull(CONVERT(varchar(10), c.g6_act_dd, 101), '')+ '; Results: ' + isnull(e.clobtext, '') AS text
FROM         b1permit a1 INNER JOIN
g6action c ON c.serv_prov_code = a1.serv_prov_code AND c.B1_PER_ID1 = a1.B1_PER_ID1 
AND c.b1_per_id2 = a1.b1_per_id2 AND 
c.b1_per_id3 = a1.b1_per_id3 AND c.g6_status &lt;&gt; 'SCHEDULED' LEFT OUTER JOIN
BACTIVITY_COMMENT e ON e.serv_prov_code = c.serv_prov_code AND e.B1_PER_ID1 = c.B1_PER_ID1 
AND e.B1_PER_ID2 = c.B1_PER_ID2 AND 
e.B1_PER_ID3 = c.B1_PER_ID3 AND e.g6_act_num = c.g6_act_num AND e.comment_type = 'Inspection Result Comment'
WHERE     a1.b1_alt_id = @id
and a1.SERV_PROV_CODE='KNOXVILLE'</CommandText>
      </Query>
      <Fields>
        <Field Name="B1_ALT_ID">
          <DataField>b1_alt_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="G6_COMPL_DD">
          <DataField>g6_compl_dd</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="TEXT">
          <DataField>text</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
