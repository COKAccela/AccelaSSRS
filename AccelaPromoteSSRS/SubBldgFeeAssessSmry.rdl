      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>c7445403-d66c-4667-948d-b2aaa869606d</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Record_ID">
            <Value>=Parameters!Record_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> select b.b1_alt_id as RECORD_ID, feeitem_seq_nbr, gf_des, gf_unit, gf_fee, 
case action_type
    when 'I' then gf_fee
    when 'V' then (gf_fee * -1)
    when 'U' then gf_fee
    when 'D' then (gf_fee * -1)
    else gf_fee
end as GF_FEE_WITH_SIGN,
format(gf_fee_apply_date, 'MM/dd/yyyy') as apply_date,
case action_type
    when 'I' then 'Assess'
    when 'V' then 'Void'
    when 'U' then 'Update'
    when 'D' then 'Delete'
    else 'Unknown'
end as ACTION,fee_comment
from f4feeitem_audit_trail a, b1permit b
where a.serv_prov_code = 'KNOXVILLE'
and b.serv_prov_code = 'KNOXVILLE'
and a.b1_per_id1 = b.b1_per_id1
and a.b1_per_id2 = b.b1_per_id2
and a.b1_per_id3 = b.b1_per_id3
and b.b1_alt_id = @Record_ID
order by b.b1_alt_id, feeitem_seq_nbr, gf_fee_apply_date, action_type;</CommandText>
      </Query>
