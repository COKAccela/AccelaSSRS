  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Comments">
            <Value>=Parameters!Comments.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
SELECT     b.B1_WORK_DESC, a.b1_alt_id, a.b1_file_dd
FROM         dbo.B1PERMIT a, dbo.BWORKDES b
WHERE     a.SERV_PROV_CODE =   'KNOXVILLE'

--b.SERV_PROV_CODE

AND a.B1_PER_ID1 = b.B1_PER_ID1 
AND a.B1_PER_ID2 = b.B1_PER_ID2 AND a.B1_PER_ID3 = b.B1_PER_ID3 
AND (CHARINDEX(upper(@Comments), upper(b.B1_WORK_DESC))&gt; 0)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
