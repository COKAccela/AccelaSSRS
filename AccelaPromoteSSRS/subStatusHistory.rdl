  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ID">
            <Value>=Parameters!ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
select B1PERMIT.b1_alt_id, 
status, 
--CONVERT(VARCHAR(23), status_history.rec_date, 121)as rec_date,
FORMAT(status_history.rec_date,'MM/dd/yyyy HH:mm')as rec_date,
--to_char(status_history.rec_date, 'MM/DD/YYYY HH24:MI') as rec_date,
status_history.status_comment, status_history.rec_ful_nam
FROM B1PERMIT, status_history
WHERE B1PERMIT.SERV_PROV_CODE = status_history.SERV_PROV_CODE
AND B1PERMIT.B1_PER_ID1       = status_history.B1_PER_ID1
AND B1PERMIT.B1_PER_ID2       = status_history.B1_PER_ID2
AND B1PERMIT.B1_PER_ID3       = status_history.B1_PER_ID3
and status_history.type = 'APPLICATION'
and B1PERMIT.SERV_PROV_CODE='KNOXVILLE'
and B1PERMIT.B1_ALT_ID = CONVERT(VARCHAR, @ID)
ORDER BY status_history.status_hist_nbr</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
