        </QueryParameters>
        <CommandText>SELECT
    [DBO].[V_INSPECTION].[DATE_INSPECTION] AS [DATE INSPECTION],
    [DBO].[V_INSPECTION].[INSPECTOR_USERID] AS [INSPECTOR USERID],
    [DBO].[V_INSPECTION].[DATE_SCHEDULED] AS [DATE SCHEDULED],
    [DBO].[V_INSPECTION].[INSPECTION_TYPE] AS [INSPECTION_TYPE],
    [DBO].[V_INSPECTION].[RESULT_] AS [RESULT_],
    [DBO].[V_INSPECTION].[COMMENTS_RESULT] AS [COMMENTS_RESULT]
FROM
    [DBO].[V_INSPECTION]
INNER JOIN
    [DBO].[V_RECORD] ON [DBO].[V_RECORD].[RECORD_ID] = [DBO].[V_INSPECTION].[RECORD_ID]
WHERE
    [DBO].[V_INSPECTION].AGENCY_ID='KNOXVILLE' and [DBO].[V_INSPECTION].[RECORD_ID] = @Record_ID
GROUP BY
    [DBO].[V_INSPECTION].[DATE_INSPECTION],
    [DBO].[V_INSPECTION].[INSPECTOR_USERID],
    [DBO].[V_INSPECTION].[DATE_SCHEDULED],
    [DBO].[V_INSPECTION].[INSPECTION_TYPE],
    [DBO].[V_INSPECTION].[RESULT_],
    [DBO].[V_INSPECTION].[COMMENTS_RESULT]
ORDER BY
    [DBO].[V_INSPECTION].[DATE_INSPECTION] ASC;</CommandText>
