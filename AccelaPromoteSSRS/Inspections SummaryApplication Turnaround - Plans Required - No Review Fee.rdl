  <DataSets>
    <DataSet Name="Details">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PermitType">
            <Value>=Parameters!PermitType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OpenedDateFrom">
            <Value>=Parameters!OpenedDateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OpenedDateTo">
            <Value>=Parameters!OpenedDateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select  W.[RECORD_ID] 
		,W.[TASK]
		,W.[STATUS]
		,W.[DATE_STATUS]
		,R.[DATE_OPENED]
		,R.[BUILDING_COUNT] 
		,R.[STATUS] as  RecordStatus 
 FROM [DBO].[V_RECORD] R
JOIN [DBO].[V_WORKFLOW_HISTORY]  W ON R.[RECORD_ID]= W.[RECORD_ID] 
where W.STATUS = 'Application Complete - Plans Required - NO REVIEW FEE'
	AND W.RECORD_STATUS NOT LIKE '%Withdrawn%'
	AND R.[RECORD_MODULE] = 'Building'
	AND  R.AGENCY_ID= 'KNOXVILLE'
	AND ('ALL' in (@PermitType) OR R.[RECORD_TYPE] in (@PermitType))
	AND TRY_CAST(R.[DATE_OPENED] AS DATE) &gt;= @OpenedDateFrom
	AND TRY_CAST(R.[DATE_OPENED] AS DATE) &lt;= @OpenedDateTo</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="RECORD_ID">
          <DataField>RECORD_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TASK">
          <DataField>TASK</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="STATUS">
          <DataField>STATUS</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DATE_STATUS">
          <DataField>DATE_STATUS</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="DATE_OPENED">
          <DataField>DATE_OPENED</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="BUILDING_COUNT">
          <DataField>BUILDING_COUNT</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="RecordStatus">
          <DataField>RecordStatus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Summary">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PermitType">
            <Value>=Parameters!PermitType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OpenedDateFrom">
            <Value>=Parameters!OpenedDateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OpenedDateTo">
            <Value>=Parameters!OpenedDateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @OpenedDateFrom as Date = '01/01/2024', @OpenedDateTo Date = '03/15/2024'

select  
	count(W.[RECORD_ID] ) as Record_Count
	,Sum(CASE WHEN DATEDIFF(DAY, DATE_OPENED, R.DATE_STATUS) = 0 THEN 0.5 ELSE DATEDIFF(DAY, DATE_OPENED, R.DATE_STATUS) END) as Total_Days
	,sum(CASE WHEN DATEDIFF(DAY, DATE_OPENED, R.DATE_STATUS) = 0 THEN 0.5 ELSE DATEDIFF(DAY, DATE_OPENED, R.DATE_STATUS) END)/count(W.[RECORD_ID])  as Average_Days
 FROM [DBO].[V_RECORD] R
JOIN [DBO].[V_WORKFLOW_HISTORY]  W ON R.[RECORD_ID]= W.[RECORD_ID] 
where W.STATUS = 'Application Complete - Plans Required - NO REVIEW FEE'
	AND W.RECORD_STATUS NOT LIKE '%Withdrawn%'
	AND R.[RECORD_MODULE] = 'Building'
	AND R.AGENCY_ID= 'KNOXVILLE'
	AND ('ALL' in (@PermitType) OR R.[RECORD_TYPE] in (@PermitType))
	AND TRY_CAST(R.[DATE_OPENED] AS DATE) &gt;= @OpenedDateFrom
	AND TRY_CAST(R.[DATE_OPENED] AS DATE) &lt;= @OpenedDateTo</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Record_Count">
          <DataField>Record_Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Total_Days">
          <DataField>Total_Days</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Average_Days">
          <DataField>Average_Days</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
