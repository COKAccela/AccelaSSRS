  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ID">
            <Value>=Parameters!ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     B1PERMIT.B1_ALT_ID, BCHCKBOX.B1_CHECKBOX_DESC, .BCHCKBOX.B1_CHECKLIST_COMMENT AS Comments
FROM         dbo.B1PERMIT, dbo.BCHCKBOX
WHERE     B1PERMIT.SERV_PROV_CODE = BCHCKBOX.SERV_PROV_CODE AND B1PERMIT.B1_PER_ID1 = BCHCKBOX.B1_PER_ID1 AND 
B1PERMIT.B1_PER_ID2 = BCHCKBOX.B1_PER_ID2 AND B1PERMIT.B1_PER_ID3 = BCHCKBOX.B1_PER_ID3 AND 
(B1PERMIT.B1_PER_TYPE = 'Work Order') AND (BCHCKBOX.B1_CHECKBOX_DESC = 'Service Request Information') AND 
(B1PERMIT.B1_ALT_ID = CONVERT(VARCHAR,@ID)) AND B1PERMIT.SERV_PROV_CODE = 'KNOXVILLE'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="B1_ALT_ID">
          <DataField>B1_ALT_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
