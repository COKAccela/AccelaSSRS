  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select a.b1_alt_id, b.rec_date, b.text
from b1permit a 
join bpermit_comment b 
on b.serv_prov_code = a.serv_prov_code and
b.b1_per_id1 = a.b1_per_id1 and
b.b1_per_id2 = a.b1_per_id2 and
b.b1_per_id3 = a.b1_per_id3 and
b.rec_status = 'A' 
where a.b1_alt_id = @id
and a.SERV_PROV_CODE='KNOXVILLE'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
