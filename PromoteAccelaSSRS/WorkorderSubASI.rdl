  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ID">
            <Value>=Parameters!ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     B1PERMIT.B1_ALT_ID, BCHCKBOX.B1_CHECKBOX_DESC, BCHCKBOX.B1_CHECKLIST_COMMENT AS ASI_SW
FROM         dbo.B1PERMIT, dbo.BCHCKBOX
WHERE     B1PERMIT.SERV_PROV_CODE = BCHCKBOX.SERV_PROV_CODE AND B1PERMIT.B1_PER_ID1 = BCHCKBOX.B1_PER_ID1 AND 
B1PERMIT.B1_PER_ID2 = BCHCKBOX.B1_PER_ID2 AND B1PERMIT.B1_PER_ID3 = BCHCKBOX.B1_PER_ID3 AND 
(BCHCKBOX.B1_CHECKBOX_TYPE = 'ASI') AND 
(B1PERMIT.B1_PER_TYPE = 'Stormwater') AND B1PERMIT.SERV_PROV_CODE = 'KNOXVILLE' AND
(B1PERMIT.B1_ALT_ID = (SELECT  max(PARENT_RECORD_ID)
FROM         dbo.V_RELATED_RECORD
WHERE  AGENCY_ID ='KNOXVILLE' AND  CHILD_RECORD_ID = CONVERT(VARCHAR,@ID)))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
