          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ENTITY">
          <DataField>ENTITY</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ATTRIBUTE">
