  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SRID">
            <Value>=Parameters!SRID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     a.B1_ALT_ID, b.B1_CHECKBOX_DESC, b.B1_CHECKLIST_COMMENT
FROM         dbo.B1PERMIT a, dbo.BCHCKBOX b
WHERE     a.SERV_PROV_CODE = b.SERV_PROV_CODE AND a.B1_PER_ID1 = b.B1_PER_ID1 AND a.B1_PER_ID2 = b.B1_PER_ID2 
AND a.B1_PER_ID3 = b.B1_PER_ID3 AND 
A.SERV_PROV_CODE='KNOXVILLE'
AND (a.B1_ALT_ID = @SRID) 
AND (b.B1_CHECKLIST_COMMENT IN ('2nd Request', '3rd Request', 'Yes', 'Checked', 'CHECKED'))</CommandText>
      </Query>
      <Fields>
        <Field Name="B1_ALT_ID">
          <DataField>B1_ALT_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="B1_CHECKBOX_DESC">
          <DataField>B1_CHECKBOX_DESC</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="B1_CHECKLIST_COMMENT">
          <DataField>B1_CHECKLIST_COMMENT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
