  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select   a.b1_alt_id, 
  b.b1_checkbox_desc, 
    b.b1_checklist_comment as intersection
from b1permit a
left join bchckbox b
  on b.serv_prov_code = a.serv_prov_code
  and b.b1_per_id1 = a.b1_per_id1
  and b.b1_per_id2 = a.b1_per_id2
  and b.b1_per_id3 = a.b1_per_id3
  and b.b1_checkbox_desc = 'Intersection'
  and b.b1_checkbox_type = 'COK_INTERNAL'
WHERE   a.SERV_PROV_CODE ='KNOXVILLE' AND a.b1_alt_id = convert(varchar,(@id))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
