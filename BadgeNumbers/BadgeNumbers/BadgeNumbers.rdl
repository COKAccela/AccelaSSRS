  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <CommandText>SELECT 
EMPL_BADGE_INFO.DEPTID as DEPTID,
EMPL_BADGE_INFO.EMPL_NAME as Name, 
EMPL_BADGE_INFO.TIME_CLOCK_BADGE_NBR as BADGE_NO, 
EMPL_BADGE_INFO.DEPTID_DESC as DEPTID_DESC, 
EMPL_BADGE_INFO.JOBCODE_DESC as JOBCODE_DESC

FROM  
	(
	SELECT 
		P.SERV_PROV_CODE SERV_PROV_CODE,
		P.B1_PER_ID1 B1_PER_ID1,
		P.B1_PER_ID2 B1_PER_ID2,  
		P.B1_PER_ID3 B1_PER_ID3,
		bb.b1_checkbox_desc, 
		bb.b1_checklist_comment
	FROM  B1PERMIT P 
	inner join BCHCKBOX BB
		on BB.SERV_PROV_CODE       = P.SERV_PROV_CODE
		  AND BB.B1_PER_ID1           = P.B1_PER_ID1
		  AND BB.B1_PER_ID2           = P.B1_PER_ID2
		  AND BB.B1_PER_ID3           = P.B1_PER_ID3
	where  P.SERV_PROV_CODE = 'KNOXVILLE'
		and P.B1_PER_GROUP          = 'AMS'
		AND P.B1_PER_TYPE             = 'Miscellaneous'
		AND P.B1_PER_SUB_TYPE         = 'Badge Info'
		AND P.b1_per_category         = 'NA'
		AND P.REC_STATUS              = 'A'
		AND P.SERV_PROV_CODE='KNOXVILLE' ) 
    AS BADGE_INFO_INNER
PIVOT  
(  
    max(b1_checklist_comment) 
	for b1_checkbox_desc IN (
		[DEPTID],
		[DEPTID_DESC], 
		[EMPL_NAME], 
		[EMPL_STATUS], 
		[EMPLID],
		[HOURLY_RT],
		[JOBCODE],
		[JOBCODE_DESC], 
		[LAST_UPDATED_DATE],
		[LAST_UPDATED_TIME],
		[TIME_CLOCK_BADGE_NBR]
		) 
	) as EMPL_BADGE_INFO</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
